<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">List of Hosted countries</h1>
    <p class="lead">Each year one country will host the summit</p>
  </div>
</div>


<table class="table">
  <thead class="thead-dark">
     
    <tr>
      <th scope="col">Year</th>
      <th scope="col">Country</th>
      <th scope="col">Continent</th>
    </tr>
  
  </thead>
  <tbody>
        {{#each @model as |venueList|}}
    <tr>
  <th scope="row">{{venueList.year}}</th>
  <td>{{venueList.country}}</td>
  <td>{{venueList.continent}}</td>
    </tr> 
     {{/each}}
  </tbody>
</table>





